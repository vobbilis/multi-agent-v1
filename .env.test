# Anthropic API Key for Claude
ANTHROPIC_API_KEY="<YOUR_ANTHROPIC_API_KEY>"

# Kubectl context for local Kind cluster
KUBECTL_CONTEXT=kind-orchestrator

# Google Gemini Key
GEMINI_API_KEY="<YOUR_GEMINI_API_KEY>"

# Open AI KEY
OPENAI_API_KEY="<YOUR_OPENAI_API_KEY>"

# LLM Configuration
LLM_PROVIDER="openai"
LLM_MODEL="gpt-4o-mini"

# React Agent Configuration
REACT_DISPLAY_THOUGHTS="true"
REACT_DISPLAY_OUTPUTS="true"
REACT_AUTO_APPROVE_ACTIONS="true" 